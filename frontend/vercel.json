{
  "version": 2,
  "buildCommand": "cd .. && npm run build --workspace=shared && cd frontend && npm run build",
  "outputDirectory": "dist",
  "installCommand": "cd .. && npm install --ignore-scripts && cd frontend",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}